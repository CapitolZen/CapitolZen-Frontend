{{bills/bill-header bill=model.bill}}
{{#ui/ui-card}}
  {{#block-slot "content"}}
    <h5>Summary</h5>
    <p>{{model.bill.summary}}</p>

    {{#if model.wrappers}}
      <h5>Saved For</h5>
      {{#each model.wrappers as |wrapper|}}
        <p>{{wrapper.group.title}} {{wrapper.position}}</p>
      {{/each}}
    {{/if}}
    
  {{/block-slot}}
{{/ui/ui-card}}