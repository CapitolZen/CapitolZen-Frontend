
<div class="container">
  <div class="group-header row">
    {{#if group.logo}}
      <div class="header-img-wrapper col-sm-1">
        <img src="{{group.logo}}" alt="{{group.title}}" class="img-fluid">
      </div>
    {{/if}}
    <div class="col-sm-10">
      <h4 class="page-title">{{group.title}}</h4>
    </div>
  </div>
</div>
{{#if group.id}}
  {{ui/group-nav group=group}}
{{/if}}
