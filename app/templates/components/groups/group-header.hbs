
<div class="container">
  <div class="group-header row">
    {{#if (unbound group.logo)}}
      <div class="header-img-wrapper col-sm-1">
        <img src="{{unbound group.logo}}" alt="{{group.title}}" class="img-fluid">
      </div>
    {{/if}}
    <div class="col-sm-10">
      <h4 class="page-title">{{unbound group.title}}</h4>
    </div>
  </div>
</div>
{{#if group.id}}
  {{ui/group-nav group=group class="mt-1"}}
{{/if}}
