{{ui-page-property "Files" property="title"}}
<div class="row">
  <div class="col-md-8 offset-md-2">
    {{#bs-tab as |tab|}}
      {{#tab.pane title="Files"}}
        {{#ui/ui-box as |b|}}
          {{#b.body}}
            {{files/file-library}}
          {{/b.body}}
        {{/ui/ui-box}}
      {{/tab.pane}}
      {{#tab.pane title="Upload"}}
        {{#ui/ui-box as |b|}}
          {{#b.body}}
            {{files/file-upload}}
          {{/b.body}}
        {{/ui/ui-box}}
      {{/tab.pane}}


    {{/bs-tab}}

  </div>
</div>